			</attributes>
		</shortcode>
		<shortcode>
			<tag>at_btn</tag>
			<attributes>
				<attribute>text</attribute>
				<attribute type="link">url</attribute>
			</attributes>
		</shortcode>
		<shortcode>
			<tag>latest_posts</tag>
			<attributes>
				<attribute>title</attribute>
				<attribute>read_more_text</attribute>
			</attributes>
